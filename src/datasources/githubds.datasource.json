{
  "name": "githubds",
  "connector": "rest",
  "baseURL": "https://api.github.com",
  "crud": false,
  "options": {
    "headers": {
      "accept": "application/json",
      "content-type": "application/json",
      "User-Agent": "loopback4-github-analytics",
      "Authorization": "${GHAUTH}"
    }
  },
  "operations": [
    {
      "template": {
        "method": "GET",
        "url": "https://api.github.com/search/issues?q=repo:strongloop/loopback-next+is:pr+merged:{startdate}..{enddate}&per_page=100"
      },
      "functions": {
        "getPRs": ["startdate", "enddate"]
      }
    }
  ]
}
